{
  "name": "knightwatcherbot",
  "description": "Get alerts via telegram for any system alerts",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://github.com/suddi/knightwatcherbot",
  "author": {
    "name": "Sudharshan Ravindran",

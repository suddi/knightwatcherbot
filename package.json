    "url": "https://suddi.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suddi/knightwatcherbot"
  },
  "bugs": {
    "url": "https://github.com/suddi/knightwatcherbot/issues"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "keywords": [],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 2.0.0"
  },
  "scripts": {
    "check-coverage": "istanbul check-coverage",
    "coverage": "istanbul cover _mocha -- --opts test/options/default.opts",
    "deploy-init": "claudia create --name knightwatcherbot --region ap-northeast-1 --api-module lib",
    "deploy": "claudia update --version production",
    "doctest": "mocha --opts test/options/doctest.opts",
    "lint": "eslint .",
    "prepush": "npm test",
    "security-check": "nsp check --output summary",
    "start": "ntl",
    "test": "npm run lint && npm run security-check && npm run doctest"
  },
  "dependencies": {
    "claudia-api-builder": "^2.0.0",
    "dynamodb-doc": "^1.0.0"
  },
  "devDependencies": {
    "claudia": "^2.6.0",
    "eslint": "^3.0.0",
    "eslint-config-suddi": "^3.0.0",
    "husky": "^0.12.0",
    "istanbul": "^0.4.0",
    "jsdoctest": "^1.0.0",
    "mocha": "^3.0.0",
    "nsp": "^2.0.0",
    "ntl": "^1.0.0"
  },
  "eslintConfig": {
    "extends": "suddi/backend"
  }
}
